mixin sMaterial(data)
	// start sMaterial
	+b.sMaterial.section#sMaterial&attributes(attributes)
		.container
			+e.title Работаем с проверенными материалами
			+e.row.row
				-for(var i = 1; i < 9; i++)
					+sMaterialItem(i)

	// end sMaterial

mixin sMaterialItem(imgNum)
	+e.col.col-6.col-md-3
		+e.img
			img(loading='lazy' src=`img/@2x/sMaterial-${imgNum}.png` alt="")